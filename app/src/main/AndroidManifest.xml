<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.wedosoft.layouts">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FrameLayout"
            android:label="@string/title_activity_absolute_layout" >
        </activity>
        <activity
            android:name=".LinearLayout"
            android:label="@string/title_activity_linear_layout" >
        </activity>
        <activity
            android:name=".RelativeLayout"
            android:label="@string/title_activity_relative_layout" >
        </activity>
        <activity
            android:name=".ListVieww"
            android:label="@string/title_activity_list_view" >
        </activity>
        <activity
            android:name=".GridVieww"
            android:label="@string/title_activity_grid_view" >
        </activity>
        <activity
            android:name=".WebVieww"
            android:label="@string/title_activity_web_view" >
        </activity>
    </application>

</manifest>